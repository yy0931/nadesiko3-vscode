{
	"name": "nadesiko3-vscode",
	"displayName": "日本語プログラミング言語 なでしこv3",
	"description": "",
	"version": "1.2.2",
	"license": "MIT",
	"publisher": "yy0931",
	"repository": {
		"type": "git",
		"url": "https://github.com/yy0931/nadesiko3-vscode"
	},
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:nadesiko3"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "nadesiko3",
				"extensions": [
					".nako"
				],
				"configuration": "./language/language-configuration.json"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "nadesiko3.runFile",
					"when": "editorLangId == nadesiko3",
					"group": "1_run@10"
				}
			],
			"commandPalette": [
				{
					"command": "nadesiko3.runFile",
					"when": "editorLangId == nadesiko3"
				}
			]
		},
		"commands": [
			{
				"command": "nadesiko3.runFile",
				"title": "Run Active File",
				"category": "Nadesiko3",
				"icon": "$(play)"
			},
			{
				"command": "nadesiko3.compileFile",
				"title": "Compile Current File",
				"category": "Nadesiko3"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"test": "TS_NODE_FILES=\"true\" mocha -r ts-node/register 'src/**/*.test.ts'"
	},
	"devDependencies": {
		"@types/chai": "^4.2.14",
		"@types/fs-extra": "^9.0.6",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.14.22",
		"@types/node-fetch": "^2.5.8",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"chai": "^4.2.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-node": "^9.1.1",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"browserslist": "^4.16.1",
		"caniuse-db": "^1.0.30001180",
		"csv-lite-js": "^0.0.7",
		"dayjs": "^1.10.4",
		"express": "^4.17.1",
		"fs-extra": "^9.1.0",
		"html": "^1.0.0",
		"iconv-lite": "^0.6.2",
		"marked": "^1.2.8",
		"node-fetch": "^2.6.1",
		"opener": "^1.5.2",
		"readline": "^1.3.0",
		"readline-sync": "^1.4.10",
		"sendkeys-js": "^0.0.4"
	}
}
